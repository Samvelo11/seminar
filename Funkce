using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Funkce
{
    internal class Program
    {
     
        static void WriteNumber(int number)
        {
            Console.WriteLine(number);
            if (number < 5000)
            {
            WriteNumber(number + 1);
            }

        }
        static void Main(string[] args)
        {
          WriteNumber(0);
        }
    }
}
